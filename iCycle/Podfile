# Uncomment this line to define a global platform for your project

platform :ios, '9.0'
workspace ‘iCycle’
use_frameworks!

target 'iCycle' do

  pod 'MySqlSwiftNative', '~> 1.0.6'
  pod 'CommonCrypto', '~> 1.1'

end

post_install do |installer|
  `rm -rf Pods/Headers/Private`
  `find Pods -regex ‘Pods\/.*\.modulemap' -print0 | barges -0 sed -i '' `
end	